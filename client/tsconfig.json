{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "outDir": "./dist",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "Shared/*": [
        "src/Shared/*"
      ]
    },
    "lib": [
      "es2017",
      "dom",
      "esnext"
    ]
  },
  "include": [
    "./src/**/*"
  ]
}